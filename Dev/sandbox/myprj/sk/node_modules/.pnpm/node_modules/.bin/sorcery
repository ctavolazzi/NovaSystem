#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/sorcery@0.11.0/node_modules/sorcery/bin/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/sorcery@0.11.0/node_modules/sorcery/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/sorcery@0.11.0/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/sorcery@0.11.0/node_modules/sorcery/bin/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/sorcery@0.11.0/node_modules/sorcery/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/sorcery@0.11.0/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sorcery/bin/sorcery" "$@"
else
  exec node  "$basedir/../sorcery/bin/sorcery" "$@"
fi
