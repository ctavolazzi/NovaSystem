#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/@sveltejs+kit@1.15.7_svelte@3.58.0_vite@4.3.1/node_modules/@sveltejs/kit/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/@sveltejs+kit@1.15.7_svelte@3.58.0_vite@4.3.1/node_modules/@sveltejs/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/@sveltejs+kit@1.15.7_svelte@3.58.0_vite@4.3.1/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/@sveltejs+kit@1.15.7_svelte@3.58.0_vite@4.3.1/node_modules/@sveltejs/kit/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/@sveltejs+kit@1.15.7_svelte@3.58.0_vite@4.3.1/node_modules/@sveltejs/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/@sveltejs+kit@1.15.7_svelte@3.58.0_vite@4.3.1/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
