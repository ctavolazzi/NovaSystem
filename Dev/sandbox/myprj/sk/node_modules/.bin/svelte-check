#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/svelte-check@3.2.0_sass@1.62.0_svelte@3.58.0/node_modules/svelte-check/bin/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/svelte-check@3.2.0_sass@1.62.0_svelte@3.58.0/node_modules/svelte-check/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/svelte-check@3.2.0_sass@1.62.0_svelte@3.58.0/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/svelte-check@3.2.0_sass@1.62.0_svelte@3.58.0/node_modules/svelte-check/bin/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/svelte-check@3.2.0_sass@1.62.0_svelte@3.58.0/node_modules/svelte-check/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/svelte-check@3.2.0_sass@1.62.0_svelte@3.58.0/node_modules:/Users/ctavolazzi/Code/NovaSystem/dev/sandbox/myprj/sk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
